<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vv.finance.investment.bg.stock.move.mapper.MoveThemeMapper">

    <insert id="saveBatch">
        insert into t_stock_move_theme (`time`, `code`, `name`, `move_type`, `move_data`, `create_time`, `update_time`)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.time},#{item.code},#{item.name},#{item.moveType},#{item.moveData}, CURRENT_TIMESTAMP,CURRENT_TIMESTAMP)
        </foreach>

    </insert>
</mapper>
